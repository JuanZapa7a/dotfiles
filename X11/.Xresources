!Character Fonts
URxvt*font: xft:MesloLGS NF:pixelsize=18:antialias=true
URxvt*letterSpace: -0
! windows default
URxvt*borderLess: false
URxvt*externalBorder: 0
URxvt*internalBorder: 4
URxvt*scrollBar: false

URxvt*saveLines: 5000

! Keymaps
URxvt.iso14755: false
URxvt.iso14755_52: false


URxvt*perl-ext-common: default,matcher,tabbedex,resize-font

! to open links in the default web browser with the left mouse button, 

URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! resize-font decimal point
URxvt.resize-font.step: 1

! To control tabs use:
! Key 	Description
! Shift+Down 	New tab
! Shift+Left 	Go to left tab
! Shift+Right 	Go to right tab
! Shift+Up      Rename tab
! Ctrl+Left 	Move tab to the left
! Ctrl+Right 	Move tab to the right
! Ctrl+d 	    Close tab
! 

!URxvt*keysym.Control-Shift-N: perl:tabbedex:new_tab
!URxvt*keysym.Control-Shift-K: perl:tabbedex:next_tab
!URxvt*keysym.Control-Shift-J: perl:tabbedex:prev_tab
!URxvt.keysym.Control-Shift-R: perl:tabbedex:rename_tab
!URxvt.keysym.Control-Shift-H: perl:tabbedex:move_tab_left
!URxvt.keysym.Control-Shift-L: perl:tabbedex:move_tab_right

URxvt*tabbedex.tabbar-fg: 2 !foreground bar5
URxvt*tabbedex.tabbar-bg: 0 !background bar
URxvt*tabbedex.tab-fg: 3 !foregroundn tab
URxvt*tabbedex.tab-bg: 0 !background tab
URxvt*tabbedex.transparent:true
URxvt*tabbedex.new-button: false
URxvt.tabbedex.reopen-on-close:no
URxvt*tabbedex.title:false
